<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="491.0" prefWidth="669.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.Logincontroller">
  <children>
    <Label layoutX="125.0" layoutY="40.0" text="Log In To Hall Management System" textFill="#001066">
      <font>
        <Font name="System Bold" size="25.0" />
      </font>
    </Label>
    <TextField fx:id="username" layoutX="218.0" layoutY="183.0" prefWidth="239.0" />
    <PasswordField fx:id="pass" layoutX="218.0" layoutY="273.0" prefWidth="239.0" />
    <Label layoutX="218.0" layoutY="150.0" text="USERNAME">
      <font>
        <Font name="System Bold" size="15.0" fx:id="x1" />
      </font>
    </Label>
    <Label font="$x1" layoutX="218.0" layoutY="245.0" text="PASSWORD" />
    <Button font="$x1" layoutX="218.0" layoutY="319.0" mnemonicParsing="false" onAction="#Actionlogin" text="LOG IN" />
    <Button font="$x1" layoutX="326.0" layoutY="319.0" mnemonicParsing="false" onAction="#Actioncancel" text="CANCEL" />
  </children>
</AnchorPane>
